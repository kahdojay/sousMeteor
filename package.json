{
  "name": "sous-meteor",
  "version": "0.0.1",
  "description": "---",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start-development": "meteor run --settings settings-development.json",
    "start-staging": "MONGO_URL=\"$(node -p 'settings=require(\"./settings-staging.json\");settings.MONGO_URL')\" meteor run --settings settings-staging.json",
    "start-production": "MONGO_URL=\"$(node -p 'settings=require(\"./settings-production.json\");settings.MONGO_URL')\" meteor run --settings settings-production.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy-staging": "modulus env set METEOR_SETTINGS \"$(cat settings-staging.json)\" -p 'SousStaging' && modulus deploy -p 'SousStaging'",
    "deploy-production": "modulus env set METEOR_SETTINGS \"$(cat settings-production.json)\" -p 'SousProduction' && modulus deploy -p 'SousProduction'"
  },
  "dependencies": {
    "node-slack": "0.0.7"
    },
  "devDependencies": {
    "ddp-client": "^0.1.2",
    "ws": "^0.8.0"
  }
}
